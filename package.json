{
  "name": "elderjs-template",
  "version": "1.3.2",
  "description": "This is a project template for [Elder.js](https://elderguide.com/tech/elderjs/) apps. The template lives at https://github.com/elderjs/template and the Elder.js source is here: https://github.com/elderjs/elderjs",
  "main": "./src/build.js",
  "scripts": {
    "start": "npm run dev",
    "build": "node ./src/cleanPublic.js && npm run build:rollup && npm run build:html && echo '' => ./docs/.nojekyll",
    "build:rollup": "rollup -c",
    "build:html": "node ./src/build.js",
    "dev": "rollup  -c -w --no-watch.clearScreen",
    "serve": "node ./src/cleanPublic.js && npm run build:rollup && NODE_ENV=production node ./src/server.js",
    "esbuild": "node esbuild",
    "eslint": "eslint --ext .js .",
    "eslint:fix": "eslint --fix --ext .js .",
    "prettier:svelte": "prettier --write --plugin-search-dir=. ./**/*.svelte",
    "p:serve": "npm run build && python3 -m http.server --directory ./docs 3000"
  },
  "type": "commonjs",
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@elderjs/elderjs": "^1.7.5",
    "@elderjs/plugin-browser-reload": "^1.3.10",
    "@elderjs/plugin-markdown": "^1.3.10",
    "@elderjs/plugin-seo-check": "^1.3.10",
    "@elderjs/plugin-sitemap": "^1.3.10",
    "@testing-library/dom": "^8.18.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@testing-library/user-event": "^14.4.3",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "elderjs-plugin-blog-pagination": "^0.7.0",
    "flexsearch": "^0.7.21",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "immutable": "^4.0.0-rc.14",
    "intersection-observer": "^0.12.0",
    "katex": "^0.16.0",
    "mermaid-parse": "^2.0.1",
    "polka": "^0.5.2",
    "sirv": "^1.0.11",
    "svelte": "^3.49.0",
    "svelte-preprocess": "^4.10.7",
    "vitest": "^0.23.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "@types/mermaid": "^9.1.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.7",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.7.2",
    "shiki": "^0.10.1",
    "shiki-themes": "^0.2.7",
    "typescript": "^4.7.4"
  },
  "author": "Nick Reese",
  "license": "MIT",
  "files": [
    "index.js",
    "elderjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elderjs/template.git"
  },
  "keywords": [
    "svelte",
    "elderjs",
    "ssr",
    "static",
    "site",
    "generator",
    "seo",
    "ssg",
    "template",
    "partial",
    "hydration"
  ],
  "bugs": {
    "url": "https://github.com/Elderjs/template/issues"
  },
  "homepage": "https://github.com/Elderjs/template#readme"
}
